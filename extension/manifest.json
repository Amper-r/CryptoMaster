{
    "description": "Расширение CryptoMaster получает данные сертификатов с пк пользователя",
    "manifest_version": 3,
    "name": "CryptoMaster",
    "version": "1.1",
    "icons": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "64": "icon-64.png",
      "128": "icon-128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "externally_connectable": {
      "matches": ["*://frontend-vladislav.ru/*"]
    },
    "permissions": ["nativeMessaging"]
  }